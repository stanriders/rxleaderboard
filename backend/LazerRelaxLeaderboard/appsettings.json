{
  "Logging": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System.Net.Http": "Warning",
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
        "Hangfire": "Warning"
      }
    },
    "Using": [ "Serilog.Enrichers.ClientInfo" ],
    "Enrich": [
      "FromLogContext",
      "WithClientAgent",
      {
        "Name": "WithClientIp",
        "Args": { "xForwardHeaderName": "CF-Connecting-IP" }
      },
      {
        "Name": "WithRequestHeader",
        "Args": { "headerName": "CF-IPCountry" }
      },
      {
        "Name": "WithRequestHeader",
        "Args": { "headerName": "Referer" }
      },
      {
        "Name": "WithRequestHeader",
        "Args": { "headerName": "User-Agent" }
      }
    ],
    "Properties": {
      "Application": "RX Leaderboard"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:dd.MM.yyyy HH:mm:ss.fff}] [{Level}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "./logs/main.log",
          "outputTemplate": "[{Timestamp:dd.MM.yyyy HH:mm:ss.fff}] [{Level}] {Message:lj}{NewLine}{Exception}",
          "rollingInterval": "Month",
          "retainedFileCountLimit": 5,
          "restrictedToMinimumLevel": "Warning"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://seq:5341"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "PathBase": "/api",
  "osuApi": {
    "ClientId": 0,
    "ClientSecret": "",
    "CallbackUrl": ""
  },
  "Database": {
    "Database": "rxleaderboard",
    "Host": "localhost",
    "Port": 5432,
    "Username": "postgres",
    "Password": "postgres"
  },
  "BeatmapCachePath": "beatmapcache",
  "ScoreQueryInterval": 750
}
